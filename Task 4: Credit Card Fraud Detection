    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "import seaborn as sns\n",
        "import matplotlib.pyplot as plt\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.metrics import classification_report, confusion_matrix\n",
        "\n",
        "# 1. Load Data directly from a working URL\n",
        "url = \"https://raw.githubusercontent.com/nsethi31/Kaggle-Data-Credit-Card-Fraud-Detection/master/creditcard.csv\"\n",
        "df = pd.read_csv(url)\n",
        "print(\"Dataset Loaded Successfully!\")\n",
        "\n",
        "# 2. Preprocess (As required by Task 4)\n",
        "scaler = StandardScaler()\n",
        "df['Amount'] = scaler.fit_transform(df[['Amount']]) # Scaling transaction amount [cite: 19, 20]\n",
        "\n",
        "# 3. Model Setup\n",
        "X = df.drop(['Class', 'Time'], axis=1) # Using anonymized variables [cite: 19]\n",
        "y = df['Class']\n",
        "\n",
        "# 4. Handle Class Imbalance\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, y, test_size=0.2, stratify=y, random_state=42)\n",
        "model = LogisticRegression(class_weight='balanced', max_iter=1000) # Suitable classification model\n",
        "model.fit(X_train, y_train)\n",
        "\n",
        "# 5. Evaluate\n",
        "y_pred = model.predict(X_test)\n",
        "print(\"\\n--- Final Performance Metrics ---\")\n",
        "print(classification_report(y_test, y_pred))\n",
        "\n",
        "# Visual evidence for your submission\n",
        "plt.figure(figsize=(6, 4))\n",
        "sns.heatmap(confusion_matrix(y_test, y_pred), annot=True, fmt='d', cmap='Reds')\n",
        "plt.title('Task 4: Confusion Matrix')\n",
        "plt.show()"
      ],
